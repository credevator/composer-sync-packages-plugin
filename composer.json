{
  "name": "credevator/composer-sync-packages-plugin",
  "description": "Sync packages and repositories between Composer projects.",
  "type": "composer-plugin",
  "require": {
    "composer-plugin-api": "^2.0",
    "symfony/console": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Credevator\\ComposerSyncPackagesPlugin\\": "src/"
    }
  },
  "require-dev": {
    "composer/composer": "^2.0"
  },
  "extra": {
    "class": "Credevator\\ComposerSyncPackagesPlugin\\Plugin"
  }
}
